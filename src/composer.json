{
    "name": "paycrypto-me/woocommerce-gateway-paycrypto-me",
    "description": "WooCommerce payment gateway for PayCrypto.Me (accept crypto payments)",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "classmap": [
            "includes/",
            "exceptions/"
        ]
    },
    "authors": [
        {
            "name": "Lucas Rosa",
            "email": "lucas.rosa95br@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.1.0",
    "config": {
        "audit": {
            "ignore": [
                "PKSA-j43q-24zh-tyzv",
                "PKSA-36gf-zqdd-tq5m"
            ]
        },
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "lucas-rosa95/bitcoin": "1.0.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lucas-rosa95/bitcoin-php"
        }
    ]
}
